{
  "name": "react-github",
  "version": "1.0.0",
  "description": "shows a user's profile from github",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beezy12/react-github.git"
  },
  "eslintConfig": {
    "env": {
      "mocha": true,
      "node": true,
      "es6": true,
      "browser": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "res": true,
      "app": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 0,
      "no-undef": 0,
      "no-extra-semi": 0,
      "no-redeclare": 0,
      "comma-dangle": 0,
      "no-mixed-spaces-and-tabs": 0,
      "no-extra-boolean-cast": 0,
      "no-empty": 0
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
  },
  "author": "Brian Stumbaugh <brian.stumbaugh@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beezy12/react-github/issues"
  },
  "homepage": "https://github.com/beezy12/react-github#readme"
}
